<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.mit.printAtMIT"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="10" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application android:icon="@drawable/ic_launcher" 
                 android:label="@string/app_name" 
                 android:debuggable="true">
        <uses-library android:name="com.google.android.maps" />
        
        <activity
            android:name=".PrintAtMITActivity"
            android:label="print@mit" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
 	    <activity android:name=".main.LoginActivity"></activity>
        <activity android:name=".main.MainMenuActivity"></activity>
        <activity android:name=".main.SettingsActivity"></activity>
        <activity android:name=".print.PrintDownloadsActivity"></activity>
        <activity android:name=".print.PrintMenuActivity"></activity>
        <activity android:name=".print.PrintOptionsActivity"></activity>
        
        <activity android:name=".view.PrinterMapActivity" android:launchMode="singleTop"  android:process="map.test">
            <action android:name="android.intent.action.MAIN" />
        </activity>
        
        <activity android:name=".view.PrinterInfoActivity"></activity>
        <activity android:name=".view.PrinterListActivity"></activity>
    </application>

</manifest>